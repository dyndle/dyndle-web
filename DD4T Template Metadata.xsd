            <xsd:appinfo>
              <ExtensionXml xmlns="http://www.tridion.com/ContentManager/5.0"></ExtensionXml>
              <tcm:DefaultNumericValue xmlns:tcm="http://www.tridion.com/ContentManager/5.0">12</tcm:DefaultNumericValue>
