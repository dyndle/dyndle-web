<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings xdt:Transform="InsertIfMissing">
		<!-- Image enhancement settings -->
		<add key="ImageEnhancement.LocalPath" value="/EnhancedImages" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
		<add key="ImageEnhancement.BackgroundColor" value="#f00" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
		<add key="ImageEnhancement.CacheSeconds" value="20" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
	</appSettings>
</configuration>